#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <

        &kp ESC &kp Q &kp W &kp E &kp R
        &kp LCTRL &kp A &kp S &kp D &kp F
        &kp Z &kp X &kp C &kp LSHFT &kp SPACE

            >;
        };
    };
};